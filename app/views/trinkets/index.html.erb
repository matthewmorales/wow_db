<p id="notice"><%= notice %></p>

<h1>Listing Trinkets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Quality</th>
      <th>Itemlevel</th>
      <th>Armor</th>
      <th>Context</th>
      <th>Icon</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trinkets.each do |trinket| %>
      <tr>
        <td><%= trinket.name %></td>
        <td><%= trinket.quality %></td>
        <td><%= trinket.itemLevel %></td>
        <td><%= trinket.armor %></td>
        <td><%= trinket.context %></td>
        <td><%= trinket.icon %></td>
        <td><%= link_to 'Show', trinket %></td>
        <td><%= link_to 'Edit', edit_trinket_path(trinket) %></td>
        <td><%= link_to 'Destroy', trinket, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
