<p id="notice"><%= notice %></p>

<h1>Listing Rings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Icon</th>
      <th>Quality</th>
      <th>Itemlevel</th>
      <th>Armor</th>
      <th>Context</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rings.each do |ring| %>
      <tr>
        <td><%= ring.name %></td>
        <td><%= ring.icon %></td>
        <td><%= ring.quality %></td>
        <td><%= ring.itemLevel %></td>
        <td><%= ring.armor %></td>
        <td><%= ring.context %></td>
        <td><%= link_to 'Show', ring %></td>
        <td><%= link_to 'Edit', edit_ring_path(ring) %></td>
        <td><%= link_to 'Destroy', ring, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
